{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:start": "serve storybook-static",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,mjs,css}\"",
    "format:fix": "prettier --write  \"src/**/*.{ts,tsx,js,jsx,mjs,css}\"",
    "lint": "next typegen && eslint",
    "lint:staged": "lint-staged",
    "lint:css": "stylelint \"src/**/*.{ts, tsx, js, jsx, mjs, css}\"",
    "lint:fix": "stylelint \"src/**/*.{ts, tsx, js, jsx, mjs, css}\" --fix && next typegen && eslint --fix",
    "type-check": "next typegen && tsc --noEmit",
    "commitlint": "commintlint --edit",
    "build-storybook": "storybook build"

  },
  "dependencies": {
    "next": "15.5.6",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@cspell/eslint-plugin": "^9.2.0",
    "@eslint/eslintrc": "^3",
    "@storybook/addon-a11y": "^9.1.13",
    "@storybook/addon-docs": "^9.1.13",
    "@storybook/addon-onboarding": "^9.1.13",
    "@storybook/addon-vitest": "^9.1.13",
    "@storybook/nextjs": "^9.1.13",
    "@stylistic/eslint-plugin": "^5.2.3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.6",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-storybook": "^9.1.13",
    "lint-staged": "^16.1.5",
    "postcss-styled-syntax": "^0.7.1",
    "prettier": "^3.6.2",
    "storybook": "^9.1.3",
    "stylelint": "^16.23.1",
    "stylelint-config-standard": "^39.0.0",
    "tailwindcss": "^4",
    "typescript": "^5",
    "typescript-eslint": "^8.41.0"
  }
}
